{
  "name":              "dreamfactory/dfe-ops-client",
  "description":       "DreamFactory Enterprise(tm) Console Operations Client",
  "homepage":          "https://www.dreamfactory.com/",
  "keywords":          [
    "dreamfactory",
    "fabric",
    "utility",
    "queue",
    "common",
    "enterprise"
  ],
  "license":           [
    "Apache-2.0"
  ],
  "type":              "library",
  "authors":           [
    {
      "name":  "Jerry Ablan",
      "email": "jerryablan@dreamfactory.com"
    }
  ],
  "repositories":      [
    {
      "type": "vcs",
      "url":  "git@github.com:dreamfactorysoftware/dfe-audit.git"
    },
    {
      "type": "vcs",
      "url":  "git@github.com:dreamfactorysoftware/dfe-database.git"
    },
    {
      "type": "vcs",
      "url":  "git@github.com:dreamfactorysoftware/dfe-storage.git"
    },
    {
      "type": "vcs",
      "url":  "git@github.com:dreamfactorysoftware/dfe-common.git"
    }
  ],
  "minimum-stability": "dev",
  "require":           {
    "dreamfactory/dfe-common":   "dev-develop as dev-master",
    "dreamfactory/dfe-database": "dev-develop as dev-master",
    "guzzlehttp/guzzle":         "~4.0"
  },
  "require-dev":       {
    "dreamfactory/dfe-audit": "dev-develop as dev-master",
    "laravel/framework":      "5.1.*",
    "phpunit/phpunit":        "@stable",
    "phpspec/phpspec":        "~2.1"
  },
  "autoload":          {
    "psr-4": {
      "DreamFactory\\Enterprise\\Console\\Ops\\": "src/"
    }
  },
  "config":            {
    "preferred-install": "dist"
  },
  "extra":             {
    "branch-alias": {
      "dev-develop": "0.1.x-dev"
    }
  }
}
